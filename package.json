{
  "name": "payoneerAssignment",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "a": "react-native run-android",
    "i": "react-native run-ios",
    "gc": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && pod deintegrate && cd ..",
    "clean:all": "yarn gc && yarn clean:ios",
    "a:apk": "cd android && yarn gc && ./gradlew assembleRelease",
    "a:aab": "cd android && yarn gc && ./gradlew bundleRelease",
    "a:bundle": "yarn gc && yarn a:aab",
    "pod-i": "cd ios && pod install && cd ..",
    "pod-u": "cd ios && pod update && pod install && cd ..",
    "pod:r": "cd ios && pod deintegrate && pod install && cd ..",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@reduxjs/toolkit": "^2.8.2",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-config": "^1.5.5",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.2.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
